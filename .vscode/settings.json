{
    "files.exclude": {
        "**/node_modules/**": false,
        "**/lib/**": false,
        "**/dist/**": false,
        "**/.nyc_output/**": false,
        "**/package-lock.json/**": false
    },
    "cSpell.words": [
        "codezero",
        "kubeconfig",
        "kubernetes",
        "k8s"
    ],
    "typescript.preferences.quoteStyle": "single",
    "typescript.format.semicolons": "remove",
    "barrelr.useDoubleQuotes": false,
    "barrelr.useSemiColons": false,
    "trailing-spaces.trimOnSave": true
}
